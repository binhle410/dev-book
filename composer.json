{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.1",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "*",
        "symfony/web-server-bundle": "^4.0",
        "composer/composer": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "symfony/yaml": "^2.7|^3.0|^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Bean\\Bundle\\BookBundle\\": "libraries\\bundle\\BeanBookBundle\\src/",
            "Bean\\Bundle\\CreativeWorkBundle\\": "libraries\\bundle\\BeanCreativeWorkBundle\\src/",
            "Bean\\Bundle\\DevToolBundle\\": "libraries\\bundle\\BeanDevToolBundle\\src/",
            "Bean\\Bundle\\ThingBundle\\": "libraries\\bundle\\BeanThingBundle\\src/",
            "Bean\\Component\\Book\\": "libraries\\component\\Book\\src/",
            "Bean\\Component\\CreativeWork\\": "libraries\\component\\CreativeWork\\src/",
            "Bean\\Component\\Thing\\": "libraries\\component\\Thing\\src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Bean\\Bundle\\BookBundle\\spec\\": "libraries\\bundle\\BeanBookBundle\\tests/spec/",
            "Bean\\Bundle\\CreativeWorkBundle\\spec\\": "libraries\\bundle\\BeanCreativeWorkBundle\\tests/spec/",
            "Bean\\Bundle\\ThingBundle\\spec\\": "libraries\\bundle\\BeanThingBundle\\tests/spec/",
            "Bean\\Component\\Book\\spec\\": "libraries\\component\\Book\\tests/spec/",
            "Bean\\Component\\CreativeWork\\spec\\": "libraries\\component\\CreativeWork\\tests/spec/",
            "Bean\\Component\\Thing\\spec\\": "libraries\\component\\Thing\\tests/spec/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01CAGBTT73EKG4PGKS1DCKVRXK",
            "allow-contrib": false
        }
    }
}