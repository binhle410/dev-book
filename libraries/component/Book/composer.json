{
    "name": "beancomponent/book",
    "type": "php-component",
    "description": "Provides a Component for Book Domain Model.",
    "keywords": [ "php book components" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Bean",
            "email": "peterbean@beancomponent.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "composer/composer": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.0|^6.0",
        "symfony/phpunit-bridge": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0|^4.0"
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": { "Bean\\Component\\Book\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Bean\\Component\\Book\\spec\\": "spec/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
